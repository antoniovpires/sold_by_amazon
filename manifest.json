{
  "manifest_version": 3,
  "name": "Sold By Amazon",
  "description": "This extension filters Amazon search results by products that are sold and delivered by Amazon. ",
  "permissions": ["activeTab"],
  "version": "0.0.0.1",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
        "matches": ["*://www.amazon.com.br/*"],
        "js": ["content.js"],
        "css": ["theme.css"]
    }
  ]
}